<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>SssHeader</servlet-name>
        <servlet-class>by.belstu.servlet.SssHeader</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SssHeader</servlet-name>
        <url-pattern>/SssHeader</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SssJSON</servlet-name>
        <servlet-class>by.belstu.servlet.SssJSON</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SssJSON</servlet-name>
        <url-pattern>/SssJson</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SssXML</servlet-name>
        <servlet-class>by.belstu.servlet.SssXML</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SssXML</servlet-name>
        <url-pattern>/SssXml</url-pattern>
    </servlet-mapping>
</web-app>